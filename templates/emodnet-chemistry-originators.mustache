      <!-- EMODNET CONTACT -->
      {{#contacts}}
      <gmd:pointOfContact>
        <gmd:CI_ResponsibleParty uuid="{{ EDMO_URL }}">
          <gmd:organisationName>
            <gco:CharacterString>{{ name }}</gco:CharacterString>
          </gmd:organisationName>
          <gmd:contactInfo>
            <gmd:CI_Contact>
              <gmd:phone xmlns:xslt="http://xml.apache.org/xslt">
                <gmd:CI_Telephone>
                  <gmd:voice gco:nilReason="missing">
                    <gco:CharacterString>{{ phone }}</gco:CharacterString>
                  </gmd:voice>
                  <gmd:facsimile gco:nilReason="missing">
                    <gco:CharacterString>{{ fax }}</gco:CharacterString>
                  </gmd:facsimile>
                </gmd:CI_Telephone>
              </gmd:phone>
              <gmd:address>
                <gmd:CI_Address>
                  <gmd:deliveryPoint xmlns:xslt="http://xml.apache.org/xslt">
                    <gco:CharacterString>{{ address }}</gco:CharacterString>
                  </gmd:deliveryPoint>
                  <gmd:city xmlns:xslt="http://xml.apache.org/xslt">
                    <gco:CharacterString>{{ city }}</gco:CharacterString>
                  </gmd:city>
                  <gmd:administrativeArea xmlns:xslt="http://xml.apache.org/xslt" gco:nilReason="missing">
                    <gco:CharacterString />
                  </gmd:administrativeArea>
                  <gmd:postalCode xmlns:xslt="http://xml.apache.org/xslt">
                    <gco:CharacterString>{{ zip }}</gco:CharacterString>
                  </gmd:postalCode>
                  <gmd:country xmlns:xslt="http://xml.apache.org/xslt">
                    <gco:CharacterString>{{ country }}</gco:CharacterString>
                  </gmd:country>
                  <gmd:electronicMailAddress>
                    <gco:CharacterString>{{ mail }}</gco:CharacterString>
                  </gmd:electronicMailAddress>
                </gmd:CI_Address>
              </gmd:address>
              <gmd:onlineResource xmlns:xslt="http://xml.apache.org/xslt">
                <gmd:CI_OnlineResource>
                  <gmd:linkage>
                    <gmd:URL>{{ website }}</gmd:URL>
                  </gmd:linkage>
                  <gmd:name>
                    <gco:CharacterString>web site</gco:CharacterString>
                  </gmd:name>
                  <gmd:function>
                    <gmd:CI_OnLineFunctionCode codeList="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/resources/codelist/ML_gmxCodelists.xml#CI_OnLineFunctionCode" codeListValue="information" />
                  </gmd:function>
                </gmd:CI_OnlineResource>
              </gmd:onlineResource>
            </gmd:CI_Contact>
          </gmd:contactInfo>
          <gmd:role xmlns:xslt="http://xml.apache.org/xslt">
            <gmd:CI_RoleCode codeList="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/resources/codelist/ML_gmxCodelists.xml#CI_RoleCode" codeListValue="{{ role }}" />
          </gmd:role>
        </gmd:CI_ResponsibleParty>
      </gmd:pointOfContact>
      {{/contacts}}
